{
    "pool_size": 8,
    "verbose": true,
    "box": {
        "runtime": "runc",
        "cpu": 1,
        "memory": 64,
        "network": "default",
        "writable": false,
        "volume": "%s:/sandbox:rw",
        "cap_drop": [
            "all"
        ],
        "ulimit": [
            "nofile=96"
        ],
        "nproc": 64
    },
    "step": {
        "user": "root",
        "action": "run",
        "timeout": 50,
        "noutput": 4096
    }
}